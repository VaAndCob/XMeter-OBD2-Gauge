                      2663             SETB    EEPSDA          ;START CONDITION
                      2664             SETB    EEPSCL
                      2665             CLR     EEPSDA
                      2666             CALL    CDEL
                      2667             CLR     EEPSCL
                      2668             MOV     A, #10100001B   ;DEVICE ADDRESS 1010AAAR
                      2669             CALL    IPWRB
