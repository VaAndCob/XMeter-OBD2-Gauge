                      1452             MOV     BUFFER+2, A     ;digit1
                      1453             MOV     RESULTLEN, #03
                      1454             RET
                      1455     ;------------------
                      1456     FORMULA3:                       ;direct byte
                      1457             MOV     DPTR, #0000h
****ERROR:Labelwasalreadydefined:`formula3'
