                       178             CALL    WRCMD
                       179             CALL    DEL100m
                       180             MOV     A, #LCD_SETFUNC+11000b
                       181             CALL    WRCMD
                       182             MOV     A, #LCD_SETVISIBLE+100b ; display on + nocursor + noblink
                       183             CALL    WRCMD
                       184             MOV     A, #LCD_SHIFT+0000b
                       185             CALL    WRCMD
                       186             MOV     A, #LCD_SETMODE+11b     ;Left scroll
                       187             CALL    WRCMD
